
.header {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 44px;
  background: #FFFFFF;
  z-index: 999;
}
.header-cons {
  position: relative;  
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 100%;
}
.header .back {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 28rpx;
  margin: auto;
  width: 22px;
  height: 22px;
}
.header text {
  font-family: PingFangSC-Semibold;
  font-size: 18px;
  color: #000000;
  letter-spacing: 0;
  text-align: center;
  line-height: 17px;
}
.header_ {
  height: 44px;
}

.content {
  padding: 20rpx 20rpx 120rpx 20rpx;
}
.car-info {
  position: relative;
  margin-bottom: 20rpx;
  padding: 44rpx;
  background: #FFFFFF;
  box-shadow: 0 2px 12px 0 rgba(0,0,0,0.04);
  border-radius: 4px;
}

.service {
  position: absolute;
  right: 20rpx;
  top: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  width: 90rpx;
  height: 90rpx;
  border-radius: 50%;
  background: #F6F6F7;
}
.service image {
  width: 40rpx;
  height: 34rpx;
  margin-bottom: 4rpx;
}
.service text {
  opacity: 0.3;
  font-family: PingFangSC-Regular;
  font-size: 10px;
  color: #000000;
  line-height: 14px;
}

.car-info .part1 {
  display: flex;
  align-items: center;
  margin-bottom: 40rpx;
} 
.car-info .part1 view {
  margin-right: 22rpx;
  font-family: PingFangSC-Regular;
  font-size: 16px;
  color: #000000;
  letter-spacing: 0;
  line-height: 16px;
} 
.car-info .part1 text {
  padding: 5rpx 15rpx;
  font-family: PingFangSC-Regular;
  font-size: 10px;
  color: #FFFFFF;
  letter-spacing: 0;
  line-height: 12px;
  background: rgba(253,145,5,0.5);
  border-radius: 100px;
} 

.car-info .item {
  display: flex;
  align-items: center;
  margin-bottom: 26rpx;
}
.car-info .item-last {
  margin-bottom: 0;
}
.car-info .part2 image, .car-info .part3 image {
  width: 32rpx;
  height: 32rpx;
  margin-right: 20rpx;
}
.car-info .part2 text, .car-info .part3 text {
  width: 80%;
  flex: 1;
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #000000;
  letter-spacing: -0.34px;
  line-height: 14px;
}

.car-info .dot {
  width: 14rpx;
  height: 14rpx;
  margin-right: 26rpx;
  margin-left: 10rpx;
  border-radius: 50%;
}
.car-info .green {
  position: relative;
  background: #39c67d;
}
.car-info .green::after {
  content: '';
  position: absolute;
  left: 0;
  right: 0;
  bottom: -32rpx;
  margin: auto;
  width: 2rpx;
  height: 28rpx;
  background: #D8D8D8;
}
.car-info .yellow {
  background: #ff9b41;
}

.fee-info {
  background: #FFFFFF;
  box-shadow: 0 2px 12px 0 rgba(0,0,0,0.04);
}
.fee-info .part_1 {
  padding: 22rpx 44rpx;
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #000000;
  letter-spacing: 0;
}
.fee-info .part_1 text {
  font-family: PingFangSC-Regular;
  font-size: 14px;
  letter-spacing: 0;
  line-height: 20px;
  color: #FD9105;
  text-decoration: underline;
}
.fee-info .part_2 {
  margin-bottom: 20rpx;
  padding-left: 58rpx;
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #000000;
  letter-spacing: 0;  
}

.tab-cons {
  padding: 0 66rpx;
  border-bottom: 0.5px solid #EEEEEE;
}
.tab-con {
  display: flex;
  align-items: center;
}
.tab-cons .item {
  position: relative;
  flex: 1;
  text-align: center;
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #575757;
  line-height: 44px;
}

.tab-cons .item text {
  position: absolute;
  right: 16rpx;
  top: 16rpx;
  width: 36rpx;
  height: 26rpx;
  border: 1px solid #5B91F8;
  border-radius: 100px;
  color: #5B91F8;
  line-height: 26rpx;
  font-size: 9px;
  font-family: 'PingFangSC-Regular';
  background: #fff;
}

.tab-cons .select {
  color: #5B91F8;
}
.tab-cons .select::after {
  content: '';
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  margin: 0 auto;
  width: 120rpx;
  height: 6rpx;
  background: #5B91F8;
}

.tab-cons .bot-line-container .slide-wrapper {
  width: 25%;
  padding: 0 18rpx;
  box-sizing: border-box;
  margin-left: 0%;
  transition: 0.3s;
}

.tab-cons .bot-line-container  .slide-wrapper  .slide {
  height: 4px;
  background: #5B91F8;
}

.choice-car {  
  padding: 40rpx 40rpx 40rpx 30rpx;
  background: #FFFFFF;
  box-shadow: 0 2px 12px 0 rgba(0,0,0,0.04);
}
.choice-car .item {
  position: relative;
  display: flex;
  align-items: center;
  margin-bottom: 50rpx;
}
.choice-car .left {
  position: relative;
  width: 160rpx;
  height: 120rpx;
  margin-right: 20rpx;
  border: 1px solid #E5E5E5;
}
.choice-car .left image {
  display: block;
  width: 100%;
  height: 100%;
}
.choice-car .left text {
  position: absolute;
  left: 0;
  bottom: 0;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 34rpx;
  font-family: PingFangSC-Regular;
  font-size: 11px;
  color: #FFFFFF;
  letter-spacing: -0.27px;
  text-align: center;
  line-height: 11px;
  background: rgba(0, 0, 0, 0.6);
  z-index: 10;
}
.choice-car .right {
}
.choice-car .right .part1 {
  margin-bottom: 20rpx;
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #000000;
  letter-spacing: 0;
  line-height: 14px;  
}
.choice-car .right .part2 {
  display: flex;
  align-items: center;
  margin-bottom: 20rpx;
}
.choice-car .right .part2 image {
    width: 22rpx;
    height: 22rpx;
    margin-right: 4rpx; 
}
.choice-car .right .part2 text {
  font-family: PingFangSC-Regular;
  font-size: 10px;
  color: rgba(0,0,0,0.3);
  letter-spacing: 0;
  line-height: 10px;  
}
.choice-car .right .part3 {
  font-family: PingFangSC-Regular;
  font-size: 10px;
  color: rgba(0,0,0,0.3);
  letter-spacing: 0;
  line-height: 10px;  
}
.choice-car .right .part3 text {
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #FD9105;
  letter-spacing: 0;
  line-height: 10px;
}
.choice-car .add-sub {
  position: absolute;
  right: 0;
  bottom: 10rpx;
  display: flex;
  align-items: center;
}
.choice-car .add-sub image {
  width: 52rpx;
  height: 52rpx;
}
.choice-car .add-sub text {
  min-width: 52rpx;
  height: 52rpx;
  padding: 0 10rpx;
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #000000;
  letter-spacing: 0;
  line-height: 52rpx;
  text-align: center;
}

.footer {
  position: fixed;
  left: 0;
  bottom: 0;
  display: flex;
  align-items: center;
  width: 100%;
  height: 120rpx;
  z-index: 9999;
  background: #FFFFFF;
}
.footer .left {
  flex: 1;
  padding-left: 24rpx;
  font-family: PingFangSC-Regular;
  font-size: 12px;
  color: rgba(0,0,0,0.3);
  letter-spacing: 0;
  line-height: 22px;   
}
.footer .left .part1 {
  display: flex;
}
.footer .left text {
  font-family: PingFangSC-Regular;
  letter-spacing: 0;
  line-height: 22px;
  color: rgba(253,145,5,1);
}
.footer .left .yu {
  flex: 1;
}
.footer .left .s {
  font-size: 14px;
}
.footer .left .b {
  font-size: 20px;
}
.footer .left .fees {
  display: flex;
  align-items: center;
  padding-right: 42rpx;
  font-family: PingFangSC-Regular;
  font-size: 12px;
  color: #000000;
  letter-spacing: 0;
  line-height: 12px;
}
.footer .left image {
  width: 30rpx;
  height: 30rpx;
  margin-left: 8rpx;
}

.footer .right {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  width: 300rpx;
  height: 100%;
  background-image: linear-gradient(-90deg, #FFA900 0%, #FF8000 98%);
  box-shadow: 0 5px 7px 0 rgba(0,0,0,0.15);  
}
.footer .right text {
  font-family: PingFangSC-Regular;
  font-size: 16px;
  color: #FFFFFF;
  letter-spacing: 0;
  text-align: center;
  line-height: 17px;
}


/* 计费标准 */
.slide-up_popup > .slot-wrapper > .head {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 84rpx;
  background: #FFFFFF;
}

.slide-up_popup > .slot-wrapper > .head > .text {
    font-family: PingFangSC-Regular;
    font-size: 14px;
    color: #999999;
    line-height: 14px;
}

.cont {
  margin-bottom: 40rpx;
  padding: 60rpx 60rpx 30rpx 60rpx;
}
.cont .dot {
  width: 12rpx;
  height: 12rpx;
  margin-right: 10rpx;
  border-radius: 50%;
  background: #FD9105;
}
.cont .item {
  margin-bottom: 34rpx;
}
.cont .item-last {
  margin-bottom: 0;
}
.cont .part1 {
  display: flex;
  align-items: center;
  margin-bottom: 24rpx;
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #000000;
  line-height: 14px;
}
.cont .p {
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: rgba(0,0,0,0.3);
  line-height: 16px;  
}
.slot-wrapper .tips {
  margin-bottom: 60rpx;
  padding: 0 50rpx;
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #999999;
  line-height: 16px;
}
.slot-wrapper .btn {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 88rpx;
  margin: 0 12rpx;
  font-family: PingFangSC-Regular;
  font-size: 16px;
  color: #FFFFFF;
  line-height: 16px;
}
.slot-wrapper .btn-yellow {
  background-image: linear-gradient(-90deg, #FFA900 0%, #FF8000 98%);
  box-shadow: 0 5px 7px 0 rgba(0,0,0,0.15);
  border-radius: 4px;
}
.slot-wrapper {
  padding-bottom: 40rpx;
}

.mask {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 1000;
  background: rgba(0, 0, 0, 0.6);
}
.mask .fees-con {
  position: absolute;
  left: 0;
  bottom: 120rpx;
  width: 100%;
  background: #FFFFFF;
}
.mask .head {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100rpx;
  margin-bottom: 50rpx;
}
.mask .head .text {
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #999999;
  line-height: 14px;
}
.mask .head .icon-gb {
  position:absolute;
  right:0;
  top:0;
  padding: 36rpx;
  font-size:24rpx;
  color:#666;  
}
.mask .cons {
  width: auto;
  max-height: 400rpx;
  padding-left: 80rpx;
}
.mask .cons .item {
  display: flex;
  align-items: center;
  margin-bottom: 40rpx;
}
.mask .cons .item .num {
  flex: 1;
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: rgba(0, 0, 0, 0.4);
  line-height: 14px;
}
.mask .cons .item .money {
  flex: 1;
  font-family: PingFangSC-Regular;
  font-size: 16px;
  color: #FD9105;
  line-height: 14px;  
  text-align: center;
}
.mask .cons .item .money-rg {
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: rgba(0, 0, 0, 0.4);
  line-height: 14px;
}

/* 车辆详情 */
.masks {
  position: fixed;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 9999;
  background: rgba(0, 0, 0, 0.6);
}
.masks swiper {
  height: 260px;
  margin-bottom: 40rpx;
}
.masks swiper-item {
  display: flex;
  justify-content: center;
  align-items: center;  
}
.masks image {
  width: 100%;
  height: 100%;
}
.masks .part2 {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-bottom: 60rpx;
}
.masks .part2 image {
    width: 22rpx;
    height: 22rpx;
    margin-right: 4rpx; 
}
.masks .part2 text {
  font-family: PingFangSC-Regular;
  font-size: 10px;
  color: #FFFFFF;
  letter-spacing: 0;
  line-height: 10px;  
}
.masks .part3 {
  margin-bottom: 32rpx;
  text-align: center;
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #FFFFFF;
  letter-spacing: 0;
  line-height: 14px;
}
.masks .part4 {
  width: 70%;
  margin: 0 auto 126rpx;
  font-family: PingFangSC-Regular;
  font-size: 12px;
  color: #FFFFFF;
  letter-spacing: 0;
  text-align: center;
  line-height: 24px;
}
.masks .cancel {
  display: block;
  width: 72rpx;
  height: 72rpx;
  margin: 0 auto;
}



